cmd - return : open -n /Applications/Alacritty.app
cmd - b : open -n /Applications/Google\ Chrome.app
cmd + shift - c : open -n /Applications/Visual\ Studio\ Code.app
cmd + shift - r : brew services restart yabai

cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4
cmd - 5 : yabai -m space --focus 5
cmd - 6 : yabai -m space --focus 6
cmd - 7 : yabai -m space --focus 7
cmd - 8 : yabai -m space --focus 8
cmd - 9 : yabai -m space --focus 9


cmd + lctrl - 1 : yabai -m space --focus 10
cmd + lctrl - 2 : yabai -m space --focus 11
cmd + lctrl - 3 : yabai -m space --focus 12
cmd + lctrl - 4 : yabai -m space --focus 13
cmd + lctrl - 5 : yabai -m space --focus 14
cmd + lctrl - 6 : yabai -m space --focus 15
cmd + lctrl - 7 : yabai -m space --focus 16
cmd + lctrl - 8 : yabai -m space --focus 17
cmd + lctrl - 9 : yabai -m space --focus 18

cmd - e : yabai -m window --toggle split
cmd - g : sh -c ~/.config/yabai/scripts/stack.sh

cmd - h : yabai -m window --focus west || yabai -m window --focus stack.prev
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - l : yabai -m window --focus east || yabai -m window --focus stack.next

cmd - 0x32 : yabai -m space --focus recent
cmd - 0x1B : yabai -m space --focus prev
cmd - 0x18 : yabai -m space --focus next

cmd + shift - 1 : yabai -m window --space 1
cmd + shift - 2 : yabai -m window --space 2
cmd + shift - 3 : yabai -m window --space 3
cmd + shift - 4 : yabai -m window --space 4
cmd + shift - 5 : yabai -m window --space 5
cmd + shift - 6 : yabai -m window --space 6
cmd + shift - 7 : yabai -m window --space 7
cmd + shift - 8 : yabai -m window --space 8
cmd + shift - 9 : yabai -m window --space 9

cmd + lctrl + shift - 1 : yabai -m window --space 10
cmd + lctrl + shift - 2 : yabai -m window --space 11
cmd + lctrl + shift - 3 : yabai -m window --space 12
cmd + lctrl + shift - 4 : yabai -m window --space 13
cmd + lctrl + shift - 5 : yabai -m window --space 14
cmd + lctrl + shift - 6 : yabai -m window --space 15
cmd + lctrl + shift - 7 : yabai -m window --space 16
cmd + lctrl + shift - 8 : yabai -m window --space 17
cmd + lctrl + shift - 9 : yabai -m window --space 18

cmd + lctrl + shift - h : yabai -m display --focus 1
cmd + lctrl + shift - l : yabai -m display --focus 2

cmd + shift - h : yabai -m window --swap west
cmd + shift - j : yabai -m window --swap south
cmd + shift - k : yabai -m window --swap north
cmd + shift - l : yabai -m window --swap east
