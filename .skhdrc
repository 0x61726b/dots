ctrl + shift - c : open -n -a Google\ Chrome
ctrl + shift - s : open -a Spotify
ctrl + shift - g : open -a GoLand
ctrl + shift - v : open -a Visual\ Studio\ Code
ctrl + shift - t : open -a Alacritty
ctrl + shift - o : open -a Telegram
ctrl + shift - p : open -a Discord
ctrl + shift - f : open -na Finder
ctrl + shift - d : open -a DataGrip
ctrl + shift - u : open -a Unity\ Hub
ctrl + shift - 0x21 : open -a Skype
ctrl + shift - 0x1E : open -a Slack
ctrl + shift - r : open -a Rider

# yabai
cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4
cmd - 5 : yabai -m space --focus 5
cmd - 6 : yabai -m space --focus 6
cmd - 7 : yabai -m space --focus 7
cmd - 8 : yabai -m space --focus 8
cmd - 9 : yabai -m space --focus 9

cmd - 0x32 : yabai -m space --focus recent
cmd - 0x1B : yabai -m space --focus prev
cmd - 0x18 : yabai -m space --focus next

cmd - l : yabai -m window --focus stack.next || yabai -m window --focus east
cmd - h : yabai -m window --focus stack.prev || yabai -m window --focus west
cmd - j : yabai -m space --layout $(yabai -m query --spaces --space | jq -r 'if .type == "stack" then "bsp" else "stack" end')
# cmd - k : yabai -m window --focus north 

# cmd + shift - l : yabai -m window --stack east
# cmd + shift - h : yabai -m window --stack south
# cmd + shift - j : yabai -m window --stack west
# cmd + shift - k : yabai -m window --stack north 

cmd + shift - 1 : yabai -m window --space 1
cmd + shift - 2 : yabai -m window --space 2
cmd + shift - 3 : yabai -m window --space 3
cmd + shift - 4 : yabai -m window --space 4
cmd + shift - 5 : yabai -m window --space 5
cmd + shift - 6 : yabai -m window --space 6
cmd + shift - 7 : yabai -m window --space 7
cmd + shift - 8 : yabai  -m window --space 8
cmd + shift - 9 : yabai -m window --space 9
cmd + shift - 0 : yabai -m window --space 0

cmd - m : yabai -m window --focus mouse

cmd + ctrl - f : yabai -m window --toggle zoom-fullscreen

